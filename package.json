  "scripts": {
    "prepare": "husky",
    "build": "next build",
    "dev": "next dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run runners:dev\"",
    "postinstall": "tsx src/scripts/postinstall.ts",
    "prisma:generate": "prisma generate",
    "prisma:generate-accelerate": "prisma generate --accelerate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "generate-graphql": "graphql-codegen --config codegen.yml",
    "tsc": "tsc --noEmit",
    "lint": "next lint && npm run tsc",
    "test": "cross-env SKIP_ENV_VALIDATION=true vitest",
    "test:ci": "SKIP_ENV_VALIDATION=true vitest run",
    "start": "next start",
    "semantic-release": "semantic-release",
    "lint-staged": "lint-staged",
    "typesafe-i18n": "typesafe-i18n",
    "dumpAllProjects": "tsx src/scripts/dumpAllProjects.ts --rewrite",
    "loadMainDump": "tsx src/scripts/loadProjects.ts public-dumps/main.json",
    "runners:build": "esbuild src/packages/dstruct-runner/main.ts --bundle --platform=node --target=node12 --minify --outfile=src/packages/dstruct-runner/dist/main.js",
    "runners:watch": "esbuild src/packages/dstruct-runner/main.ts --bundle --platform=node --target=node12 --sourcemap --outfile=src/packages/dstruct-runner/dist/main.js --watch",
    "runners:run": "node --watch src/packages/dstruct-runner/dist/main.js",
    "runners:dev": "concurrently \"npm run runners:watch\" \"nodemon --watch src/packages/dstruct-runner/dist/main.js src/packages/dstruct-runner/dist/main.js\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
